        mc:Ignorable="d"
        Title="Fag Mocker" Height="300" Width="300">
    <Grid>
        <Button x:Name="RecordButton" Content="レコード" HorizontalAlignment="Left" Margin="26,42,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
