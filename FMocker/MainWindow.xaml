        <Button x:Name="PlayButton" Content="プレー" HorizontalAlignment="Left" Margin="158,78,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <ListBox x:Name="ListBoxObject" HorizontalAlignment="Left" Height="134" Margin="26,126,0,0" VerticalAlignment="Top" Width="114"/>
        <TextBox x:Name="" HorizontalAlignment="Left" Height="134" Margin="158,126,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>

    </Grid>
